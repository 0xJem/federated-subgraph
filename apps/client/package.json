{
  "name": "@olympusdao/treasury-subgraph-client",
  "version": "1.0.5",
  "description": "WunderGraph client for the OlympusDAO treasury subgraph",
  "main": "./dist/client.js",
  "module": "./dist/client.mjs",
  "types": "./dist/client.d.ts",
  "exports": {
    ".": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs",
      "default": "./dist/client.js"
    }
  },
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "publish-package": "yarn publish --access public"
  },
  "dependencies": {
    "@wundergraph/sdk": "^0.151.0"
  },
  "devDependencies": {
    "@olympusdao/treasury-subgraph": "*",
    "tsup": "^6.7.0",
    "zod": "^3.21.4"
  }
}